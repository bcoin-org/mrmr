{
  "name": "mrmr",
  "version": "0.1.0",
  "description": "Murmur3 for javascript",
  "keywords": [
    "murmur3"
  ],
  "license": "MIT",
  "repository": "git://github.com/bcoin-org/mrmr.git",
  "homepage": "https://github.com/bcoin-org/mrmr",
  "bugs": {
    "url": "https://github.com/bcoin-org/mrmr/issues"
  },
  "author": "Christopher Jeffrey <chjjeffrey@gmail.com>",
  "main": "./lib/mrmr.js",
  "scripts": {
    "install": "node-gyp rebuild || echo 'Build failed.'",
    "browserify": "browserify -s mrmr lib/mrmr.js | uglifyjs -c > mrmr.js",
    "clean": "rm -f mrmr.js",
    "lint": "eslint lib/ test/ || exit 0",
    "test": "mocha --reporter spec test/*-test.js",
    "webpack": "webpack --mode production --config webpack.config.js"
  },
  "dependencies": {
    "bindings": "~1.3.0",
    "nan": "~2.10.0"
  },
  "devDependencies": {
    "babelify": "^8.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "browserify": "^16.2.2",
    "eslint": "^4.19.1",
    "mocha": "^5.2.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "uglify-es": "^3.3.9",
    "webpack": "^4.11.1",
    "webpack-cli": "^3.0.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "gypfile": true,
  "browser": {
    "./lib/murmur3": "./lib/murmur3-browser.js"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
